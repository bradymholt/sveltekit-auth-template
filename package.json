{
  "name": "svelte-auth",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "deploy": "npm run build && wrangler pages publish --env production --project-name sveltekit-auth-template .svelte-kit/cloudflare",
    "logs": "wrangler pages deployment tail --project-name sveltekit-auth-template"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/adapter-cloudflare": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@tsndr/cloudflare-worker-jwt": "^2.1.4",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "postcss-nested": "^6.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "zod": "^3.20.2"
  }
}
